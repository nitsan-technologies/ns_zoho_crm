{
  "name": "nitsan/ns-zoho-crm",
  "type": "typo3-cms-extension",
  "description": "One of the only TYPO3 extension which provides to integrate features of Zoho like add leads all information from typo3 site.",
  "homepage": "https://extensions.typo3.org/extension/ns_zoho_crm/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "ZOHO",
    "zoho",
    "Nitsan"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_zoho_crm/issues"
  },
  "require": {
    "typo3/cms-core": ">=7.6.0",
    "in2code/powermail": ">=2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsZohoCrm\\": "Classes/"
    }
  },
  "replace": {
      "ns_zoho_crm": "self.version",
      "nitsan/ns-zoho-crm": "self.version"
  },
  "extra": {
      "typo3/cms": {
          "extension-key": "ns_zoho_crm"
      }
  }
}